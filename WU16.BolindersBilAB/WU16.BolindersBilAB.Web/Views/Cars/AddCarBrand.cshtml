@model AddBrandViewModel

@{
    ViewData["Title"] = "AddBrand";
}

<h2>Skapa ny bilmärke</h2>
<hr />
<div class="container" >
    <div class="col-md-4">
        <form asp-action="AddCarBrand"enctype="multipart/form-data" >
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <input type="file" asp-for="Image" class="form-control" multiple />
            </div>
            <div class="form-group">
                <label asp-for="BrandName" class="control-label"></label>
                <input asp-for="BrandName" class="form-control" />
                <span asp-validation-for="BrandName" class="text-danger"></span>
                
            </div>
            <div class="form-group">
                <input type="submit" value="Create" class="btn btn-default" />
            </div>
        </form>
    </div>
    </div>
    <div>
        <a asp-action="Index">Back to List</a>
    </div>


